<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.project.mapper.master.CardTypeMapper">
    <resultMap id="BaseResultMap" type="com.project.entity.CardType">
        <id column="id_card_type" property="idCardType" jdbcType="INTEGER"/>
        <result column="name" property="name" jdbcType="VARCHAR"/>
    </resultMap>
    <sql id="Base_Column_List">
       id_card_type, `name`
	</sql>
    <insert id="insertSelective" parameterType="com.project.entity.CardType">
        insert into card_type
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="idCardType != null">
                id_card_type,
            </if>
            <if test="name != null and name != ''">
                `name`
            </if>
        </trim>
        <trim prefix="values(" suffix=")" suffixOverrides=",">
            <if test="idCardType != null">
                #{idCardType,jdbcType=INTEGER},
            </if>
            <if test="name != null and name != ''">
                #{name,jdbcType=VARCHAR}
            </if>
        </trim>
    </insert>
    <select id="selectAll" resultMap="BaseResultMap">
        select <include refid="Base_Column_List"/> from `card_type`
    </select>
</mapper>